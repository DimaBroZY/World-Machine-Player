[sub_resource type="GDScript" id="GDScript_obkdo"]
script/source = "extends PanelContainer

var info := true
var info_window = null
var player: AudioStreamPlayer



func _ready() -> void:
		var saved_info = Settings.get_setting(\"is_info_message_appear\", true)
		info = saved_info
		
		var saved = Settings.get_setting(\"is_info_message_appear\", null)
		if saved == null:
			info = true
		else:
			info = saved
			
		

		player = AudioStreamPlayer.new()
		add_child(player)
		player.volume_db = -10

func _on_purple_theme_button_pressed() -> void: # фиолетовый
	TintManager.set_tint(Color.from_rgba8(148, 101, 255, 255))  


func _on_white_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(255, 255, 255, 255)) 


func _on_blue_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(116, 107, 230, 255)) 


func _on_cyan_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(52, 205, 210, 255))


func _on_yellow_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(255, 210, 106, 255))
	
	
func _on_green_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(63, 213, 100, 255))


func _on_red_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(213, 33, 106, 255))


func _on_pink_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(254, 85, 191, 255))


func _on_orange_theme_button_pressed() -> void:
	TintManager.set_tint(Color.from_rgba8(253, 143, 77, 255))


func _on_rainbow_theme_button_pressed() -> void:
	if info == true:
		if info_window != null and is_instance_valid(info_window):
			info_window.show()
			return
		# Загружаем сцену с Window как корневой нодой
		info_window = preload(\"res://Windows/info_window.tscn\").instantiate()

		# Добавляем Window в дерево сцен
		get_tree().root.add_child(info_window)
	
		info_window.close_requested.connect(func(): 
			info_window.queue_free()
			info_window = null
		)
		info_window.show()
		player.stream = preload(\"res://sfx/twm_notif4.wav\")
		player.play()
		TintManager.apply_tint_to_scene()

	else:
		TintManager.enable_rainbow(true)
"

